{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts",
        "coverage/lcov-report/*.js"
      ]
    },
    "rules": {
      "no-unused-variable": true,
      "noUnusedLocals": true,
      "no-console": true,
      "jsx-no-lambda": true
    }
  }
  